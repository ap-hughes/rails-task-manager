<h1>My Tasks</h1>
<div class="container">
  <ul>
    <% @tasks.each do |task| %>
    <li><%= link_to task.title, task_path(task) %></li>
      <% if task.completed %>
        <input type="checkbox" checked disabled>
      <% else %>
        <input type="checkbox" disabled>
      <% end %>
    <% end %>
  </ul>
</div>
<div>
  <p><strong><%= link_to "Create a new task?", tasks_new_path %></strong></p>
</div>
